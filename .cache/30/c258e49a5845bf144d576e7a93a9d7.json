idth;\n  border-radius: $radius;\n  background-color: $bg-color;\n}\n","../src/styles/reset.scss":"html,\nbody,\ndiv,\nspan,\napplet,\nobject,\niframe,\nh1,\nh2,\nh3,\nh4,\nh5,\nh6,\np,\nblockquote,\npre,\na,\nabbr,\nacronym,\naddress,\nbig,\ncite,\ncode,\ndel,\ndfn,\nem,\nimg,\nins,\nkbd,\nq,\ns,\nsamp,\nsmall,\nstrike,\nstrong,\nsub,\nsup,\ntt,\nvar,\nb,\nu,\ni,\ncenter,\ndl,\ndt,\ndd,\nol,\nul,\nli,\nfieldset,\nform,\nlabel,\nlegend,\ntable,\ncaption,\ntbody,\ntfoot,\nthead,\ntr,\nth,\ntd,\narticle,\naside,\ncanvas,\ndetails,\nembed,\nfigure,\nfigcaption,\nfooter,\nheader,\nhgroup,\nmenu,\nnav,\noutput,\nruby,\nsection,\nsummary,\ntime,\nmark,\naudio,\nvideo {\n  margin: 0;\n  padding: 0;\n  border: 0;\n  font-size: 100%;\n  font: inherit;\n  vertical-align: baseline;\n}\n\n/* HTML5 display-role reset for older browsers */\n\narticle,\naside,\ndetails,\nfigcaption,\nfigure,\nfooter,\nheader,\nhgroup,\nmenu,\nnav,\nsection {\n  display: block;\n}\n\nbody {\n  line-height: 1;\n}\n\nol,\nul {\n  list-style: none;\n}\n\nblockquote,\nq {\n  quotes: none;\n}\n\nblockquote {\n  &:before,\n  &:after {\n    content: \"\";\n    content: none;\n  }\n}\n\nq {\n  &:before,\n  &:after {\n    content: \"\";\n    content: none;\n  }\n}\n\ntable {\n  border-collapse: collapse;\n  border-spacing: 0;\n}\n\n* {\n  box-sizing: border-box;\n}\n","../src/styles/drop.scss":".drop {\n  @include base-style(100vh, 100vw, $mainBackground);\n  position: relative;\n  &__logo {\n    @include base-style(100px, 200px);\n    object-fit: contain;\n    padding: 0;\n    position: absolute;\n    left: 5%;\n  }\n\n  &__element {\n    @include base-style(100%, 80%);\n    display: flex;\n    justify-content: space-around;\n    margin: 0 auto;\n  }\n  &__card {\n    @include base-style(100%, 40%, white, 3%);\n    box-shadow: 0 5px 5px rgba(black, 0.3);\n    padding: 3%;\n  }\n  &__text {\n    height: 50%;\n    > span {\n      @include font(0.6em, 300);\n    }\n  }\n  &__zone {\n    margin: 0 auto;\n    border: 3px dashed #aaa;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    flex-direction: column;\n    @include base-style(50%, 100%, $mainBackground, 1%);\n    > form > label {\n      @include font(1em);\n    }\n\n    &:hover {\n      border: 3px solid #aaa;\n    }\n  }\n  &__danger {\n    background: red;\n    border: 3px dashed red;\n\n    &::after {\n      content: \"There a problem check your file type/size\";\n      @include font(1em);\n    }\n  }\n  &__title {\n    margin: 0;\n    margin-bottom: 3%;\n    @include font(30px, 700);\n  }\n  &__result {\n    > .drop__text {\n      margin-top: 15%;\n      > p {\n        @include font(1em);\n\n        > span {\n          font-style: italic;\n          @include font(0.8em);\n\n          &.file-upload {\n            font-style: normal;\n            @include font(1em);\n          }\n        }\n        &:first-child {\n          margin-bottom: 10%;\n        }\n      }\n    }\n  }\n\n  &__history {\n    @include base-style(30%, 80%, white, 1%);\n    margin: 0 auto;\n    margin-top: 5%;\n    padding: 3%;\n    box-shadow: 0 3px 5px rgba(black, 0.3);\n    overflow-y: scroll;\n    @include font(0.9em);\n\n    > table {\n      border-collapse: collapse;\n      width: 100%;\n      td,\n      th {\n        border: 1px solid #ddd;\n        padding: 1%;\n      }\n      th {\n        background-color: $mainBackground;\n        @include font(1.2em, $weight: 600);\n      }\n    }\n  }\n}\n","../src/styles/style.scss":"@import \"reset\";\n@import \"utils-style\";\n@import \"drop\";\n\n.container {\n  padding-top: 8%;\n  height: 55%;\n}\n"},"lineCount":null}},"error":null,"hash":"fd56470b0e58e51739099e8424056a8f","cacheData":{"env":{}}}